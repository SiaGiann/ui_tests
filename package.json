{
  "name": "nightwatch-viewport",
  "version": "1.0.24",
  "description": "UI testing for Viewport platform",
  "main": "index.js",
  "engines": {
    "node": "4.4.6"
  },
  "scripts": {
    "test": "nightwatch --env local",
    "ie": "nightwatch -e ie11",
    "sauce": "nightwatch -e chrome,ie11,android_s4_emulator,iphone_6_simulator",
    "viewport": "nightwatch --config nightwatch.conf.BASIC.js",
    "dev": "nodemon -w test/index.html --delay 250ms --exec 'npm run upload'"
  },
  "repository": {
    "type": "mercurial",
    "url": ""
  },
  "author": "A. Giannaki",
  "license": "MIT",
  "dependencies": {
    "nightwatch": "^0.9.9",
    "saucelabs": "^1.3.0",
    "selenium-download": "^2.0.7"
  },
  "devDependencies": {
    "aws-sdk": "^2.7.13",
    "chromedriver": "^2.33.2",
    "env2": "^2.1.1",
    "mime-types": "^2.1.13",
    "nodemon": "^1.11.0",
    "selenium-server": "^3.8.1"
  },
  "custom_commands_path": "./test/custom-commands",
  "keywords": [
    "nightwatch",
    "testing",
    "front-end",
    "UI",
    "browser",
    "test",
    "testing",
    "User Acceptance Testing",
    "UAT",
    "Viewport",
    "Radial SG"
  ]
